string 1 & string 2


import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str1=sc.next();
        String str2=sc.next();
        System.out.println(str1+" "+ "Technologies" +" "+ str2);
    }
}

